  </ItemGroup>
  <!-- Microsoft CSharp targets -->
  <Import Project="$(MSBuildBinPath)\Microsoft.CSharp.targets" />
  <!-- Custom BeforeClean -->
  <Target Name="BeforeClean" DependsOnTargets="CleanTestResults" />
